#include <iostream>
using namespace std;

int main()
{
    int a,b;
    int i = 0;
    while(cin>>a>>b){
        if(b!=0){
            if(i!=0){
                cout<<" ";
            }
            cout<<a*b<<" "<<b-1;
            i++;

        }


    }
    return 0;
}
