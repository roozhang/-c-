#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main()
{
    string str={101,'0'};//we only use str[2] to str[100],but I have to have 101 strings
    int n;
    cin>>n;
    vector<int> number = {n};
    for(int i = 0;i!=n;i++)
    {
        cin>>number[i];
    }
    for(int i = 0;i!= n;i++)
    {
        int k = number[i];
        if(!str[k])
        {
            for(;k!=1;)
            {

                if(k/2==1) k=3*k-1;
                else k/=2;
                str[k]=1;
            }

        }
    }
    vector<int> vec;
    for(int i = 0;i != n;i++)
    {
        int k = number[i];
        if(!str[k])
            vec.push_back(k);
    }
    for(unsigned i = 0;i!=vec.size()-1;i++)
        cout<<vec[i]<<" ";
    cout<<vec[vec.size()-1];
    return 0;
}
//写得累死了，下午刚考完数学，，回去休息吧。。
