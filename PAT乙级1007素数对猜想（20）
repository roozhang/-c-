#include <iostream>
using namespace std;
bool isprime(int a)
{
    if (a<2) return false;
    for(int i = 2;i!=a;i++)
    {
        if(a%i==0)return false;
    }
    return true;
}

int main()
{
    int n;cin>>n;
    int sum = 0;
    for(int i = 3;i!=n-2;i++)
    {
        if(isprime(i)&&isprime(i+2))
            sum++;
    }
    cout<<sum;
    return 0;
}
